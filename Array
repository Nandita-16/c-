#include <stdio.h>

struct Student {
    int roll_no;
    char name[20];
};

int main() {
    struct Student class[3] = {
        {101, "Alice"},
        {102, "Bob"},
        {103, "Charlie"}
    };
    
    for(int i = 0; i < 3; i++) {
        printf("Roll: %d, Name: %s\n", class[i].roll_no, class[i].name);
    }
    return 0;
}
